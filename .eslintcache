 [type, setType] = React.useState('');\r\n  const history = useHistory();\r\n  const classes = useStyles();\r\n\r\n  const insertData = (e) =>{\r\n    e.preventDefault()\r\n    swal(\"กด ok เพื่อยืนยันการบันทึก\",{\r\n    })\r\n    .then((value) => {\r\n      if (value) {\r\n    axios.post(\"http://localhost:3001/insert\",{\r\n      title:title,\r\n      content:content,\r\n      Date:date,\r\n      type:type\r\n    }).then(\r\n          setTitle(''),\r\n          setContent(''),\r\n          swal(\"Good job!\", \"You clicked the button!\", \"success\").then(\r\n            history.push(\"/HomeStore\"),\r\n            history.go()\r\n          )\r\n        )\r\n      }\r\n    })\r\n  }\r\n  const UpdateData = () =>{\r\n    swal(\"กด ok เพื่อยืนยันการแก้ไข\",{\r\n    })\r\n    .then((value) => {\r\n      if (value) {\r\n    axios.post(\"http://localhost:3001/updateAnnoun\",{\r\n      title:title,\r\n      content:content,\r\n      Date:date,\r\n      type:type,\r\n      id:props.id,\r\n    }).then(\r\n          setTitle(''),\r\n          setContent(''),\r\n          swal(\"Good job!\", \"You clicked the button!\", \"success\").then(\r\n            history.push(\"/HomeStore\"),\r\n            history.go()\r\n          )\r\n        )\r\n      }\r\n    })\r\n  }\r\n  const handleChange = (event) => {\r\n    setType(event.target.value);\r\n  };\r\n  const transitions = useTransition(props.open, {\r\n    from: { opacity: 0, y: 800 },\r\n    enter: { opacity: 1, y: 0 },\r\n    leave:  { opacity: 0,y: 800}\r\n  })\r\n  useEffect(() => {\r\n    const today = new Date();\r\n    const yeartoday = today.getFullYear() \r\n    const month = today.getMonth() +1\r\n    const date = today.getDate() \r\n    if(month < 10 && date > 9){\r\n      const forday = `${yeartoday}-0${month}-${date}`\r\n      setDate(forday) \r\n    }\r\n    else if(date < 10 && month > 9){\r\n      const forday = `${yeartoday}-${month}-0${date}`\r\n      setDate(forday)     \r\n    }\r\n    else if(date < 10 