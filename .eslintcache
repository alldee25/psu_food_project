lue\r\n      }\r\n      return inputfild;\r\n    })\r\n  }\r\n  const InsertData = (e) =>{\r\n    e.preventDefault(e.target.value)\r\n    const id = scores.every((score)=>{\r\n        return score.score !== ''\r\n      })\r\n     const sum =  scores.reduce((result,score)=>{\r\n        return result + Number(score.score)\r\n      },0)\r\n      if (id && time !== '' && dateInput !== '') {\r\n        if (sum>=36 && scores[4].score > 0) {\r\n          swal(\"กด ok เพื่อยืนยันการบันทึก\",{\r\n        })\r\n        .then((value) => {\r\n          if (value) {\r\n            axios.post(\"http://localhost:3001/InsertStoreCleanLevel\",{\r\n          scores:scores,\r\n          date:dateInput,\r\n          time:time,\r\n          storeId:dataCleanlinessLevelList[0].store_id,\r\n          adminId:auth.usersData[0].id,\r\n          status:'ผ่าน',\r\n          feedback:feedback \r\n          }).then(swal({\r\n            title: \"บันทึกเรียบร้อย\",\r\n            text: \"กดปุ่มเพื่อไปต่อ\",\r\n            icon: \"success\",\r\n            button: \"OK\",\r\n          }).then((value) =>{\r\n            history.push('/HomeStore/CleanlinessLevel')\r\n            history.go() \r\n            }))\r\n          }\r\n          })\r\n        } else {\r\n          swal(\"กด ok เพื่อยืนยันการบันทึก\",{\r\n          })\r\n          .then((value) => {\r\n            if (value) {\r\n              axios.post(\"http://localhost:3001/InsertStoreCleanLevel\",{\r\n            scores:scores,\r\n            date:dateInput,\r\n            time:time,\r\n            storeId:dataCleanlinessLevelList[0].store_id,\r\n            adminId:auth.usersData[0].id,\r\n            status:'ไม่ผ่าน',\r\n            feedback:feedback \r\n            }).then(swal({\r\n              title: \"บันทึกเรียบร้อย\",\r\n              text: \"กดปุ่มเพื่อไปต่อ\",\r\n              icon: \"success\",\r\n              button: \"OK\",\r\n            }).then((value) =>{\r\n              history.push('/HomeStore/CleanlinessLevel')\r